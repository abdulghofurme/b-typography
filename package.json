{
  "name": "b-typography",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "sass:dev": "sass --watch src/typography.scss:dist/typography.css",
    "sass:build": "sass src/typography.scss:dist/typography.css",
    "sass:build-vars": "sass src/variable.scss:dist/variable.css",
    "sass:build-minified": "sass src/typography.scss:dist/typography.min.css --style compressed",
    "sass:build-vars-minified": "sass src/variable.scss:dist/variable.min.css --style compressed",
    "build": "npm run sass:build && npm run sass:build-vars && npm run sass:build-minified && npm run sass:build-vars-minified",
    "prepare": "husky install"
  },
